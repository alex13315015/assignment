<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ryan9025.todo.dao.CalendarDao">
    <insert id="calendarTodo" parameterType="CalendarDto">
        insert into calendar values (
        calendar_seq.nextval,
        #{title},
        #{startDate},
        #{endDate},
        #{backgroundColor}
        )
    </insert>

    <select id="getCalendarTodo" parameterType="CalendarDto">
        select * from calendar
    </select>
</mapper>